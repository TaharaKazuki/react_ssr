{
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "plugins": ["prettier", "react", "jsx-a11y", "import"],
  "extends": [
    "prettier",
    "prettier/flowtype",
    "prettier/react",
    "airbnb"
  ],
  "rules": {
    "quotes": ["error", "single"],
    "react/jsx-filename-extension": 0,
    "no-return-assign": 0,
    "no-param-reassign": [2, { "props": false }]
  }
}