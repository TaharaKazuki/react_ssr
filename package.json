{
  "name": "react_redux_ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "start": "./app/bin/startServer.sh",
    "dev": "run-p dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/server/bundle.js\"",
    "dev:build-server": "better-npm-run build:dev:server",
    "dev:build-client": "better-npm-run build:dev:client"
  },
  "betterScripts": {
    "build:dev:server": {
      "command": "webpack --progress --colors --config webpack.server.babel.js --watch",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:dev:client": {
      "command": "webpack --progress --colors --config webpack.client.babel.js --watch",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "license": "MIT",
  "dependencies": {
    "@storybook/cli": "^3.4.4",
    "autoprefixer": "^8.5.0",
    "axios": "0.16.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-object-rest-spread": "^0.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-root-slash-import": "^1.1.0",
    "better-npm-run": "^0.1.0",
    "compression": "1.7.0",
    "concurrently": "3.5.0",
    "css-loader": "^0.28.11",
    "express": "4.15.4",
    "express-http-proxy": "1.0.6",
    "getstorybook": "^1.7.0",
    "lodash": "4.17.4",
    "node-sass": "^4.9.0",
    "nodemon": "1.12.0",
    "npm-run-all": "4.1.1",
    "postcss-loader": "^2.1.5",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.6",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "sass-loader": "^7.0.1",
    "serialize-javascript": "1.4.0",
    "webpack": "3.5.6",
    "webpack-dev-server": "2.8.2",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0"
  },
  "devDependencies": {
    "prettier": "^1.12.1"
  }
}
